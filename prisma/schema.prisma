generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Users {
  id       Int    @id @default(autoincrement())
  name     String @db.VarChar(50)
  lastname String @db.VarChar(50)
  email    String @unique @db.VarChar(75)
  password String @db.VarChar(60)
}